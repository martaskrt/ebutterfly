{
    "model": {
        "num_classes": 629,
        "name": "attention_resnet",
        "dropout": 0.2
    },
    "optimizer": {
        "learning_rate": 4,
        "weight_decay": 6,
        "lr_scheduler": {
            "step_size": 20,
            "gamma": 0.1
        }
    },
    "dataset": {
        "name": "butterfly",
        "csv_root": "data/all_images_split_Species",
        "image_dir": "data/ob_with_images/",
        "masks_dir": "data/masks/",
        "train_info_file": "train_annotations.csv",
        "test_info_file": "test_annotations.csv",
        "fine_grained_level": "Species",
        "preprocessing": {
            "normalization": {
                "resize": {
                    "scale": [
                        448,
                        448
                    ],
                    "keep_ratio": true
                }
            },
            "augmentation": {
                "random_rotation": {
                    "angle": 15
                },
                "random_crop": {
                    "proportion": 0.8
                },
                "random_flip": {
                    "probability": 0.5
                }
            }
        }
    },
    "training": {
        "batch_size": 32,
        "epochs": 50,
        "class_weights_alpha": 0.5,
        "loss": {
            "type": "guided_attention",
            "params": {}
        }
    }
}
